        DEPLIBS = $(DEPXLIB)
LOCAL_LIBRARIES = $(XLIB)
  SYS_LIBRARIES = -lm
        DEFINES = -DPOSIX
           SRCS = xlander.c globals.c initialize.c functions.c \
     instrument.c game.c resource.c
